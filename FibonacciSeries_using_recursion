#include<stdio.h>
int fibonacci(int n) {
	if(n<=1)
	return n;
	else
	return fibonacci(n-1) + fibonacci(n-2);
}
	
	int main() {
		int n,i;
		printf("Enter the number of terms you want : \n");
		scanf("%d", &n);
		for(i=0; i<n; i++) {
	  int fibo = fibonacci(i);

	  printf("\n%d", fibo);
}
		
	return 0;
}
